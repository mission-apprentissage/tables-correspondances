{
  "name": "@mission-apprentissage/tco-service-node",
  "version": "1.0.0-042f98d0629f39dc9215670a017f464a.1",
  "description": "[MNA] Tables de correspondances",
  "license": "SEE LICENSE IN LICENSE.md",
  "repository": "https://github.com/mission-apprentissage/tables-correspondances.git",
  "private": false,
  "sideEffects": false,
  "author": "MNA",
  "main": "lib/sdk/index.js",
  "types": "lib/sdk/index.d.ts",
  "publishConfig": {
    "access": "restricted"
  },
  "scripts": {
    "start": "node src/index.js",
    "debug": "nodemon --inspect=0.0.0.0 --signal SIGINT --ignore tests/ src/index.js",
    "test": "mocha --recursive --exit --timeout 5000 tests/",
    "coverage": "nyc --reporter=html --temp-dir .coverage/.nyc_output --report-dir .coverage mocha --recursive --exit --timeout 500 tests/",
    "lint": "eslint src/ tests/",
    "jobs": "node -r dotenv/config src/jobs/index.js",
    "job": "TABLES_CORRESPONDANCES_LOG_TYPE=stderr run=true node -r dotenv/config",
    "seed": "yarn job -- src/jobs/seed/index.js",
    "clear": "yarn job -- src/jobs/clear/index.js",
    "stats": "yarn job -- src/jobs/stats/index.js",
    "cfdHander": "yarn job -- src/logic/handlers/cfdHandler.js",
    "domainesMetiers": "yarn job -- src/jobs/domaines-metiers/index.js",
    "bcn:downloader": "yarn job -- src/jobs/bcnDownloader/index.js",
    "bcn:importer": "yarn job -- src/jobs/bcnImporter/index.js",
    "rncp:importer": "yarn job -- src/jobs/rncpImporter/index.js",
    "etablissements:duplicate": "yarn job -- src/jobs/oneshot/mergeEtablissementDuplicates/index.js",
    "convention:importer": "yarn job -- src/jobs/convetionFilesImporter/index.js",
    "onisep:importer": "yarn job -- src/jobs/OnisepImporter/index.js",
    "etablissements:updater": "yarn job -- src/jobs/EtablissementsUpdater/index.js",
    "es:index": "yarn job -- src/jobs/esIndex/index.js",
    "generate:swaggerschema": "yarn job -- src/jobs/generateSwaggerSchema/index.js",
    "sdk:build": "tsc -b; yarn sdk:postbuild",
    "sdk:postbuild": "cpy --cwd=src --parents '**/*.json' '**/*.csv' ../lib/",
    "sdk:clean": "tsc -b --clean && rm -rf lib",
    "sdk:prepublishOnly": "yarn build",
    "npm:release": "semantic-release",
    "npm:release:local": "CI=true LOCAL_RELEASE=true semantic-release"
  },
  "dependencies": {
    "@azure/storage-blob": "12.3.0",
    "@elastic/elasticsearch": "7.6.1",
    "aws-sdk": "2.785.0",
    "axios": "0.21.1",
    "body-parser": "1.19.0",
    "boom": "7.3.0",
    "bunyan": "1.8.12",
    "bunyan-mongodb-stream": "0.0.3",
    "bunyan-prettystream": "0.1.3",
    "bunyan-slack": "0.0.10",
    "byline": "5.0.0",
    "cli-progress": "3.9.0",
    "command-line-args": "5.1.1",
    "commander": "6.2.1",
    "compose-middleware": "5.0.1",
    "config": "3.3.1",
    "convert-csv-to-json": "1.0.2",
    "convert-csv-to-json-latin": "1.0.6",
    "csv": "5.3.2",
    "csv-parse": "4.14.1",
    "deep-object-diff": "1.1.0",
    "deepmerge": "4.2.2",
    "dotenv": "8.2.0",
    "elasticsearch-scroll-stream": "1.2.1",
    "env-var": "7.0.0",
    "express": "4.17.1",
    "faker": "5.1.0",
    "fast-luhn": "2.0.0",
    "form-data": "4.0.0",
    "fs-extra": "9.0.0",
    "iconv-lite": "0.6.2",
    "joi": "17.2.1",
    "json2csv": "5.0.1",
    "jsonwebtoken": "8.5.1",
    "lodash": "4.17.15",
    "moment": "2.26.0",
    "mongoose": "5.13.7",
    "mongoose-paginate": "5.0.3",
    "mongoose-schema-jsonschema": "1.4.7",
    "multipipe": "4.0.0",
    "oleoduc": "0.5.11",
    "passport": "0.4.1",
    "passport-http": "0.3.0",
    "passport-jwt": "4.0.0",
    "passport-local": "1.0.0",
    "query-string": "6.13.8",
    "rate-limiter-flexible": "2.2.1",
    "require-all": "3.0.0",
    "sha512crypt-node": "0.1.0",
    "ssh2-sftp-client": "6.0.1",
    "swagger-jsdoc": "6.0.1",
    "swagger-ui-express": "4.1.6",
    "title-case": "3.0.2",
    "uuid": "8.3.2",
    "xlsx": "0.16.1",
    "xml2js": "0.4.23"
  },
  "engines": {
    "node": ">=14.15.0"
  },
  "prettier": {
    "printWidth": 120,
    "bracketSpacing": true,
    "trailingComma": "es5"
  },
  "devDependencies": {
    "@semantic-release/changelog": "^5.0.1",
    "@semantic-release/exec": "^5.0.0",
    "@semantic-release/git": "^9.0.0",
    "@typescript-eslint/eslint-plugin": "^4.14.1",
    "@typescript-eslint/parser": "^4.14.1",
    "axios-mock-adapter": "1.19.0",
    "axiosist": "0.9.0",
    "cpy-cli": "3.1.1",
    "eslint": "^7.18.0",
    "eslint-config-prettier": "^7.2.0",
    "eslint-config-sbs": "^1.0.1",
    "eslint-plugin-compat": "^3.9.0",
    "eslint-plugin-eslint-comments": "^3.2.0",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-jsx-a11y": "^6.4.1",
    "eslint-plugin-mocha": "8.1.0",
    "eslint-plugin-node": "11.1.0",
    "eslint-plugin-prettier": "3.3.1",
    "eslint-plugin-react": "^7.22.0",
    "eslint-plugin-react-hooks": "^4.2.0",
    "husky": ">=4",
    "lint-staged": ">=10",
    "mocha": "7.1.2",
    "nodemon": "2.0.3",
    "nyc": "15.1.0",
    "prettier": "2.0.5",
    "semantic-release": "^17.3.1",
    "semantic-release-slack-bot": "^1.7.0",
    "typescript": "^4.1.3"
  },
  "peerDependencies": {},
  "husky": {
    "hooks": {
      "pre-commit": "./scripts/preventSensibleFilesCommit.sh && lint-staged"
    }
  },
  "lint-staged": {
    "*.js": [
      "eslint --cache --fix",
      "prettier --write"
    ]
  }
}
